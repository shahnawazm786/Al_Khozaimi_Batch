package class01;

public class SecondClassForOperator {

	public static void main(String[] args) {
		
		
		// TODO Auto-generated method stub
		// Operator - Arithmetic operator (+, -, *, / , %)
		// Assignement - (=, +=, -=, *=, /=, %=)
		// Boolean operator (>, <, >=,<=, == ,!=)
		// Logical Operator ( & (Ane), |(OR), !(Not))
		int a,b,res;
		a=100;
		b=7;
		res=a/b;
		System.out.println("Devedent =>"+res);
		res=a%b;
		System.out.println("Reminder  =>"+res);

	}

}
