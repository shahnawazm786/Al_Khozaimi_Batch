package class01;

public class SecondClassForOperator {

	public static void main(String[] args) {
		
		
		// TODO Auto-generated method stub
		// Operator - Arithmetic operator (+, -, *, / , %)
		// Assignement - (=, +=, -=, *=, /=, %=)
		// Boolean operator (>, <, >=,<=, == ,!=)
		// Logical Operator ( & (Ane), |(OR), !(Not))
		int a,b,res=100;
		System.out.println(a +"  "+b+"   "+res);
		

	}

}
