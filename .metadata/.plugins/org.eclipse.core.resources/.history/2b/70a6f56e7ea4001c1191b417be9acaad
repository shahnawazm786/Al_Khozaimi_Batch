package class08;

public class PrintExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		print1();
	}
	static void print() {
		System.out.print("a");
		System.out.print("*");
		System.out.print("a");
		System.out.println();
		System.out.println("----------");
		System.out.print("a");
	}
	static void print1() {
		System.out.println("a");
		System.out.println("*a");
		System.out.println("**a");
		System.out.println("***a");
		System.out.print("***a");
			}

}
