package class07;

public class LoopExample02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//printEvenNumberApporach1();
		double res=12.0/5.0;
		System.out.println(res);
	}
	static void printEvenNumberApporach1() {
		int x=2;
		while(x<=100) {
			//System.out.print("\t"+x);
			if(x%20==0)
				System.out.println();
			System.out.print("  "+x);
			x+=2;
		}
		
	}
}
