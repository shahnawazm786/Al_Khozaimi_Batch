package class07;

public class LoopExample02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		printEvenNumberApporach1();
	}
	static void printEvenNumberApporach1() {
		int x=2;
		while(x<=100) {
			//System.out.print("\t"+x);
			System.out.print("  "+x);
			x+=2;
		}
	}
}
